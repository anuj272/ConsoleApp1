<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://ws.ips.mastercardworldwide.com/type/execution/v1" elementFormDefault="qualified" targetNamespace="http://ws.ips.mastercardworldwide.com/type/execution/v1" xmlns:xs="http://www.w3.org/2001/XMLSchema">

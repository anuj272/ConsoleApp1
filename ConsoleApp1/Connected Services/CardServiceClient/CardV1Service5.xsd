      <xs:enumeration value="SUCCESS_WARNING" />
      <xs:enumeration value="NOT_ATTEMPTED" />
      <xs:enumeration value="TIMEOUT" />
      <xs:enumeration value="ERROR" />
      <xs:enumeration value="ERROR_DIRTY" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="FaultCodeType">
